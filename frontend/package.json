{
  "name": "nextowl",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "build": "next build",
    "build:prod": "./scripts/build/build-prod.sh",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start --port 3000",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "lint:strict": "eslint . --ext .ts,.tsx,.js,.jsx --max-warnings 0",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "type-check:prod": "tsc --project tsconfig.production.json --noEmit",
    "test": "echo \"No tests specified\" && exit 0",
    "test:watch": "echo \"No tests specified\" && exit 0",
    "analyze": "ANALYZE=true next build",
    "perf": "next build && next start",
    "audit": "pnpm audit --audit-level moderate",
    "audit:fix": "pnpm audit --fix",
    "setup": "./scripts/setup/setup-env.sh",
    "deploy:dev": "./scripts/deploy/deploy.sh dev",
    "deploy:prod": "./scripts/deploy/production-deploy.sh",
    "deploy:test": "./scripts/deploy/deploy.sh test",
    "firebase:deploy": "firebase deploy --only hosting",
    "firebase:serve": "firebase serve --only hosting",
    "firebase:login": "firebase login",
    "firebase:logout": "firebase logout",
    "firebase:projects": "firebase projects:list",
    "clean": "rm -rf .next out dist node_modules/.cache",
    "clean:all": "rm -rf .next out dist node_modules/.cache node_modules pnpm-lock.yaml",
    "reinstall": "pnpm clean:all && pnpm install",
    "precommit": "pnpm type-check && pnpm lint && pnpm format:check",
    "prebuild": "pnpm type-check && pnpm lint",
    "postbuild": "echo \"Build completed successfully\"",
    "health": "curl -f http://localhost:3000 || exit 1",
    "size": "du -sh out/ || echo \"No build found\""
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/typography": "^0.5.16",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cobe": "^0.6.4",
    "critters": "^0.0.25",
    "firebase": "^12.2.1",
    "firebase-admin": "^13.5.0",
    "firebase-functions": "^6.4.0",
    "framer-motion": "^12.23.12",
    "highlight.js": "^11.11.1",
    "icons-react": "^3.7.1",
    "lenis": "^1.3.10",
    "lucide-react": "^0.542.0",
    "next": "15.5.2",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.62.0",
    "react-markdown": "^10.1.0",
    "rehype-highlight": "^7.0.2",
    "rehype-raw": "^7.0.0",
    "remark-gfm": "^4.0.1",
    "sonner": "^2.0.7",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.1.5"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-router-dom": "^5.3.3",
    "esbuild": "^0.25.0",
    "eslint": "^9",
    "eslint-config-next": "15.5.2",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "tw-animate-css": "^1.3.7",
    "typescript": "^5"
  }
}
