{
  "name": "package.json",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lint": "eslint . --ext .ts,.tsx,.js,.jsx",
    "lint:fix": "eslint . --ext .ts,.tsx,.js,.jsx --fix",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preview": "vite preview",
    "clean": "rm -rf dist node_modules/.vite",
    "prebuild": "npm run type-check && npm run lint",
    "lighthouse": "npx lighthouse http://localhost:3000 --config-path=./lighthouse.config.js --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless --no-sandbox --disable-gpu --disable-dev-shm-usage --disable-web-security --disable-features=VizDisplayCompositor --disable-background-timer-throttling --disable-backgrounding-occluded-windows --disable-renderer-backgrounding --disable-features=TranslateUI --disable-ipc-flooding-protection --remote-debugging-port=0' --chrome-path=/usr/bin/chromium-browser",
    "lighthouse:build": "npm run build && npm run preview & sleep 5 && npx lighthouse http://localhost:4173 --config-path=./lighthouse.config.js --output=html --output-path=./lighthouse-report.html --chrome-flags='--headless --no-sandbox --disable-gpu --disable-dev-shm-usage' && pkill -f 'vite preview'",
    "lighthouse:ci": "npx lighthouse http://localhost:3000 --config-path=./lighthouse.config.js --output=json --output-path=./lighthouse-report.json --chrome-flags='--headless --no-sandbox --disable-gpu --disable-dev-shm-usage'",
    "lighthouse:mobile": "npx lighthouse http://localhost:3000 --config-path=./lighthouse-mobile.config.js --output=html --output-path=./lighthouse-mobile-report.html --chrome-flags='--headless --no-sandbox --disable-gpu --disable-dev-shm-usage'",
    "test:lighthouse": "node scripts/lighthouse-test.js",
    "test:lighthouse:dev": "node scripts/lighthouse-test.js dev",
    "test:lighthouse:build": "node scripts/lighthouse-test.js build",
    "test:lighthouse:mobile": "node scripts/lighthouse-test.js mobile",
    "test:lighthouse:wsl": "node scripts/lighthouse-wsl.js",
    "test:lighthouse:wsl:dev": "node scripts/lighthouse-wsl.js dev",
    "test:lighthouse:wsl:mobile": "node scripts/lighthouse-wsl.js mobile",
    "optimize:images": "node scripts/optimize-images.js",
    "build:optimized": "npm run optimize:images && npm run build",
    "seo:optimize": "node scripts/seo-optimization.js",
    "seo:generate": "node scripts/seo-optimization.js",
    "build:seo": "npm run seo:optimize && npm run build"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tabler/icons-react": "^3.34.1",
    "@tailwindcss/vite": "^4.1.5",
    "@types/react-syntax-highlighter": "^15.5.13",
    "autoprefixer": "^10.4.21",
    "axios": "^1.10.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "firebase": "^11.8.1",
    "framer-motion": "^12.16.0",
    "katex": "^0.16.22",
    "lenis": "^1.3.9",
    "lucide-react": "^0.513.0",
    "postcss": "^8.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-helmet-async": "^2.0.5",
    "react-hook-form": "^7.56.4",
    "react-icons": "^5.5.0",
    "react-intersection-observer": "^9.16.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.6.0",
    "react-syntax-highlighter": "^15.6.6",
    "react-toastify": "^11.0.5",
    "rehype-katex": "^7.0.1",
    "remark-gfm": "^4.0.1",
    "remark-math": "^6.0.0",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.6"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/three": "^0.179.0",
    "@typescript-eslint/eslint-plugin": "^8.40.0",
    "@typescript-eslint/parser": "^8.40.0",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "lighthouse": "^12.0.0",
    "prettier": "^3.6.2",
    "tailwindcss-animate": "^1.0.7",
    "tw-animate-css": "^1.3.7",
    "vite": "^6.3.5",
    "vite-plugin-compression2": "^0.9.3"
  }
}
