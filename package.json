{
  "name": "owlai-workspace",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "frontend"
  ],
  "scripts": {
    "dev": "cd frontend && pnpm dev",
    "build": "cd frontend && pnpm build",
    "start": "cd frontend && pnpm start",
    "lint": "cd frontend && pnpm lint",
    "lint:fix": "cd frontend && pnpm lint:fix",
    "format": "cd frontend && pnpm format",
    "format:check": "cd frontend && pnpm format:check",
    "format:fix": "cd frontend && pnpm format:fix",
    "type-check": "cd frontend && pnpm type-check",
    "lint:strict": "cd frontend && pnpm lint:strict",
    "check-all": "cd frontend && pnpm check-all",
    "fix-all": "cd frontend && pnpm fix-all",
    "analyze": "cd frontend && pnpm analyze",
    "perf": "cd frontend && pnpm perf"
  },
  "devDependencies": {
    "prettier": "^3.6.2"
  }
}
